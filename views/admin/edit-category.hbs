<div class="">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-6 d-flex align-items-center">
        <div class="card-body p-4 p-lg-5 text-black">
          <div class="">
          </div>
          <form action="/admin/edit-category/{{cate._id}}" method="post" enctype="multipart/form-data">
            <div class="d-flex align-items-center mb-3 pb-1">
              {{!-- <img src="/img/core-img/sfrlogo.png" class="img-fluid" style="height:60px;width:60px;" alt=""> --}}
              <span class="h1 fw-bold mb-0">Edit Category</span>
            </div>
            {{#if catEditErr}}
            <p class="text-danger">{{catEditErr}}</p>
            {{/if}}
            <div class="form-outline mb-2">
              <input type="text" value="{{cate.category}}" name="category" id="category"
                class="form-control form-control-lg text-capitalize" required />
              <label class="form-label" for="category" required>Category Name</label>
            </div>
            <div class="pt-1">
              <button class="btn btn-dark btn-lg btn-block" type="submit">Submit</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>